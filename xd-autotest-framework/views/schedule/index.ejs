<div class="container-fluid">

  <!-- Page Heading -->
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">
        调度策略 <small>调度策略列表</small>
      </h1>
      <ol class="breadcrumb">
        <li class="active">
          <i class="fa fa-desktop"></i> 调度策略
        </li>
      </ol>
    </div>
  </div>
  <!-- /.row -->

  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="table-responsive">
            <table id="taskTable" class="table table-bordered table-hover table-striped">

              <thead>
              <tr>
                <th class="col-lg-2">任务</th>
                <th class="col-lg-3">creatTime</th>
                <th class="col-lg-4">调度策略描述</th>
                <th class="col-lg-3">修改调度策略类型</th>
              </tr>
              </thead>

              <tbody>
              <% if (data) { %>
              <% data.forEach(function(record,index){ %>
              <tr>
                <td ><%-record.Task_name%></td>
                <td><%-record.createdAt%></td>
                <td><%-record.Schedule_desc%></td>
                <td>
                  <div class="form-inline">
                    <select name="schedule_desc" class="form-control">
                      <option>不会自动执行任务脚本</option>
                      <option>每天07:30执行任务脚本</option>
                      <option>每周日22：30执行任务</option>
                      <option>每周周一到周五21:00执行任务脚本</option>
                    </select>
                    <btn name="<%-record.Task_name%>" class="btn btn-primary" onclick="changeTaskSchedule({btn:this,taskName:'<%-record.Task_name%>'})">save</btn>
                  </div>
                </td>
              </tr>
              <% }) %>
              <% } %>
              </tbody>
            </table>
          </div>
          <div class="text-right">
            <a href="#">View All Transactions <i class="fa fa-arrow-circle-right"></i></a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- /.row -->

</div>
<!-- /.container-fluid -->
<script src="/xd-autotest-framework/js/index/frame-right.js"></script>
<script src="/xd-autotest-framework/js/schedule/schedule.js"></script>
